<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_display_mim"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.zoro175.tajwid.DisplayMimActivity"
    android:orientation="vertical"
    android:background="@drawable/menu_mim_sakinah_bg">

    <Button
        android:layout_width="190dp"
        android:layout_height="45dp"
        android:id="@+id/button21"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="270dp"
        android:background="@drawable/mim_izhar_syafawi"/>

    <Button
        android:layout_marginTop="10dp"
        android:layout_width="190dp"
        android:layout_height="45dp"
        android:id="@+id/button22"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/mim_idgham_syafawi"/>

    <Button
        android:layout_marginTop="10dp"
        android:layout_width="190dp"
        android:layout_height="45dp"
        android:id="@+id/button23"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/mim_ikhfa_syafawi"/>

    <Button
        android:layout_marginTop="10dp"
        android:layout_width="190dp"
        android:layout_height="45dp"
        android:id="@+id/button24"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/laman_utama"/>

</LinearLayout>
